{
    "name": "quantic-telecom/invoices-storage",
    "license": "GPL-3.0",
    "description": "Easy way to manage the storage of your invoices",
    "authors": [
        {
            "name": "Thibaud Dauce",
            "email": "thibaud@dauce.fr"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "quantic-telecom/storage": "~1.0",
        "quantic-telecom/invoices": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "QuanticTelecom\\InvoicesStorage\\": "src/"
        },
        "psr-4": {
            "QuanticTelecom\\InvoicesStorage\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
